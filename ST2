
#include<stdio.h>
int main()
{
int a,b,c,valid = 1;
printf("Enter triangle values:- ");
scanf("%d %d %d",&a,&b,&c);
if(a<=0 || b<=0 || c<=0) valid = 0;
if((a > 100)||(b > 100)||(c > 100)) valid = 0;
if((a+b)<=c || (b+c)<=a || (c+a)<=b) valid = 0;
if(valid == 1)
{
if((a==b)&&(b==c)) printf("Equilateral Triangle");
else if((a==b)||(b==c)||(c==a)) printf("Isosceles Triangle");
else printf("Scalene Triangle");
}
else
printf("Not a Triangle");
return 0;
}
